n = int(input("Enter the Range of the numbers:- "))
sumEven = 0
sumOdd = 0

for i in range(1,n+1,+1):
    
    if i % 2 == 0:
        sumEven += i
    
    else:
        sumOdd += i
        
print("Sum of all Even in the given range is equla to ", sumEven)
print("Sum of all Odd in the given range is equla to ", sumOdd)